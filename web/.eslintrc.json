{
  "extends": ["plugin:vue/vue3-recommended", "@vue/typescript/recommended", "plugin:prettier/recommended"],
  "overrides": [
    {
      "rules": {
        "vue/no-dupe-keys": "off",
        "vue/multi-word-component-names": "off",
        "@typescript-eslint/ban-ts-comment": "off"
      },
      "files": ["*.ts", "*.tsx", "*.vue", "*.js"]
    }
  ]
}
